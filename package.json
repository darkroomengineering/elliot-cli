{
  "name": "elliot-cli",
  "version": "1.1.2",
  "author": "kensanni",
  "bin": {
    "elliot": "./bin/run"
  },
  "bugs": "https://github.com/helloiamelliot/elliot-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "@types/inquirer": "^6.5.0",
    "ansi-colors": "^4.1.1",
    "apollo-fetch": "^0.7.0",
    "clear": "^0.1.0",
    "cli-table": "^0.3.1",
    "conf": "^6.2.1",
    "cowsay": "^1.4.0",
    "execa": "^4.0.0",
    "figlet": "^1.2.4",
    "inquirer": "^7.0.6",
    "listr": "^0.14.3",
    "minimist": "^1.2.6",
    "tslib": "^1.10.0",
    "update-notifier": "^4.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.13",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "cows/cowfile",
    "scripts/*"
  ],
  "homepage": "https://github.com/helloiamelliot/elliot-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "elliot",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "helloiamelliot/elliot-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
